          <span>News</span>\n          </Link>\n          \n          <Link to=\"/analytics\" className=\"nav-item\" onClick={() => setIsOpen(false)}>\n            <FaChartBar />\n            <span>Analytics</span>\n          </Link>\n          \n          <Link to=\"/saved\" className=\"nav-item\" onClick={() => setIsOpen(false)}>\n            <FaBookmark />\n            <span>Saved</span>\n          </Link>\n\n          <div className=\"language-selector\">\n            <label htmlFor=\"language\">Select Language:</label>\n            <select \n              id=\"language\"\n              value={language}\n              onChange={(e) => handleLanguageChange(e.target.value)}\n              className=\"language-select\"\n            >\n              {languages.map((lang) => (\n                <option key={lang.code} value={lang.code}>\n                  {lang.name}\n                </option>\n              ))}\n            </select>\n          </div>\n        </div>\n      </div>\n    </nav>\n  );\n};\n\nexport default Navbar; "],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,QAAQ,KAAQ,OAAO,CACvC,OAASC,IAAI,KAAQ,kBAAkB,CACvC,OAASC,MAAM,CAAEC,OAAO,CAAEC,WAAW,CAAEC,UAAU,CAAEC,UAAU,CAAEC,UAAU,KAAQ,gBAAgB,CACjG,MAAO,cAAc,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAEtB,KAAM,CAAAC,MAAM,CAAGC,IAAA,EAA0B,IAAzB,CAAEC,gBAAiB,CAAC,CAAAD,IAAA,CAClC,KAAM,CAACE,MAAM,CAAEC,SAAS,CAAC,CAAGhB,QAAQ,CAAC,KAAK,CAAC,CAC3C,KAAM,CAACiB,QAAQ,CAAEC,WAAW,CAAC,CAAGlB,QAAQ,CAAC,IAAI,CAAC,CAE9C,KAAM,CAAAmB,UAAU,CAAGA,CAAA,GAAM,CACvBH,SAAS,CAAC,CAACD,MAAM,CAAC,CACpB,CAAC,CAED,KAAM,CAAAK,oBAAoB,CAAIC,IAAI,EAAK,CACrCH,WAAW,CAACG,IAAI,CAAC,CACjBL,SAAS,CAAC,KAAK,CAAC,CAChBF,gBAAgB,CAACO,IAAI,CAAC,CACxB,CAAC,CAED,KAAM,CAAAC,SAAS,CAAG,CAChB,CAAEC,IAAI,CAAE,IAAI,CAAEC,IAAI,CAAE,SAAU,CAAC,CAC/B,CAAED,IAAI,CAAE,IAAI,CAAEC,IAAI,CAAE,SAAU,CAAC,CAC/B,CAAED,IAAI,CAAE,IAAI,CAAEC,IAAI,CAAE,QAAS,CAAC,CAC9B,CAAED,IAAI,CAAE,IAAI,CAAEC,IAAI,CAAE,QAAS,CAAC,CAC9B,CAAED,IAAI,CAAE,IAAI,CAAEC,IAAI,CAAE,OAAQ,CAAC,CAC9B,C